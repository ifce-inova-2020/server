generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid())
  type        String
  name        String
  email       String @unique
  password    String
  campus      String

  @@map("users")
  @@unique([id, email])
}